command="if [[ \"${SSH_ORIGINAL_COMMAND}\" = 'put '* ]]; then key=$(echo -n \"${SSH_ORIGINAL_COMMAND}\" | sed -E 's/put //' | sed -E 's/ [^ ]+//'); val=$(echo -n \"${SSH_ORIGINAL_COMMAND}\" | sed -E 's/[^ ]+ [^ ]+ //'); echo -n \"${val}\" > \"sstore/${key}\"; elif [[ \"${SSH_ORIGINAL_COMMAND}\" = 'get '* ]]; then key=$(echo -n \"${SSH_ORIGINAL_COMMAND}\" | sed -E 's/put //' | sed -E 's/get //' | sed -E 's/ [^ ]+//'); cat \"sstore/${key}\"; elif [[ \"${SSH_ORIGINAL_COMMAND}\" = 'del '* ]]; then key=$(echo -n \"${SSH_ORIGINAL_COMMAND}\" | sed -E 's/del //' | sed -E 's/ [^ ]+//'); rm \"sstore/${key}\"; elif [[ \"${SSH_ORIGINAL_COMMAND}\" = 'ls' ]]; then ls sstore; fi" ssh-rsa YOUR-PUBLIC-KEY
